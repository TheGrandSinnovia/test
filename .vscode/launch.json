    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "Django: runserver",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\manage.py",
            "args": [
                "runserver",
                "--nothreading"
            ],
            "django": true,
            "subProcess": true
        },

        {
            "name": "Django: makemigrations",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "makemigrations",
            ],
            "django": true
        },

        {
            "name": "Django: migrate",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "migrate",
            ],
            "django": true
        },

        {
            "name": "Django: collectstatic",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "collectstatic",
            ],
            "django": true
        },

        {
            "name": "Python: compilemessages",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "compilemessages",
            ],
            "django": true
        },

        {
            "name": "Python: makemessages -l es (create/update .po)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "makemessages",
                "-l",
                "es",
            ],
            "django": true
        },
    ]
}
